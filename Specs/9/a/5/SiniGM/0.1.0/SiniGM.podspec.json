{
  "name": "SiniGM",
  "version": "0.1.0",
  "summary": "基于 OpenSSL 封装 SM2、SM4 加解密、SM3 摘要算法、SM2 签名验签、ECDH 密钥协商、ASN1 编码解码、DER 编码解码，HMAC 算法等。",
  "description": "Build OpenSSL for iOS and OS X, and add sm2, sm3, sm4 header files。",
  "homepage": "https://git.sinitek-corp.com/fcs/mobile/devopsgroup/ios/sinigm.git",
  "license": "MIT",
  "authors": {
    "ky.zhang": "ky.zhang@sinitek.com"
  },
  "source": {
    "git": "https://git.sinitek-corp.com/fcs/mobile/devopsgroup/ios/sinigm.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "13.0"
  },
  "pod_target_xcconfig": {
    "ARCHS[sdk=iphonesimulator*]": "$(ARCHS_STANDARD_64_BIT)"
  },
  "source_files": "SiniGM/Classes/**/*",
  "public_header_files": "SiniGM/Classes/**/*.h",
  "frameworks": "Security",
  "dependencies": {
    "GMOpenSSL": [

    ]
  },
  "requires_arc": true,
  "static_framework": true
}
